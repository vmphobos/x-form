{
    "name": "vmphobos/xform",
    "description": "Reusable Laravel Form Components for Livewire and standard Blade.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Vasilis Milopoulos",
            "email": "vasilis.milopoulos@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Vmphobos\\XForm\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Vmphobos\\XForm\\XFormServiceProvider"
            ]
        }
